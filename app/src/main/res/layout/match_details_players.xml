<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#161621">

    <include layout="@layout/match_details_team_one_player"
        android:id="@+id/teamOnePlayerOne"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <include layout="@layout/match_details_team_one_player"
        android:id="@+id/teamOnePlayerTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/teamOnePlayerOne"
        app:layout_constraintStart_toStartOf="parent" />

    <include layout="@layout/match_details_team_one_player"
        android:id="@+id/teamOnePlayerThree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/teamOnePlayerTwo"
        app:layout_constraintStart_toStartOf="parent" />

    <include layout="@layout/match_details_team_one_player"
        android:id="@+id/teamOnePlayerFour"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/teamOnePlayerThree"
        app:layout_constraintStart_toStartOf="parent" />

    <include layout="@layout/match_details_team_one_player"
        android:id="@+id/teamOnePlayerFive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/teamOnePlayerFour"
        app:layout_constraintStart_toStartOf="parent" />

    <include layout="@layout/match_details_team_two_player"
        android:id="@+id/teamTwoPlayerOne"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/teamOnePlayerOne"/>

    <include layout="@layout/match_details_team_two_player"
        android:id="@+id/teamTwoPlayerTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/teamTwoPlayerOne"
        app:layout_constraintStart_toEndOf="@id/teamOnePlayerTwo"/>

    <include layout="@layout/match_details_team_two_player"
        android:id="@+id/teamTwoPlayerThree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/teamTwoPlayerTwo"
        app:layout_constraintStart_toEndOf="@id/teamOnePlayerThree"/>

    <include layout="@layout/match_details_team_two_player"
        android:id="@+id/teamTwoPlayerFour"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/teamTwoPlayerThree"
        app:layout_constraintStart_toEndOf="@id/teamOnePlayerFour"/>

    <include layout="@layout/match_details_team_two_player"
        android:id="@+id/teamTwoPlayerFive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/teamTwoPlayerFour"
        app:layout_constraintStart_toEndOf="@id/teamOnePlayerFive"/>

</androidx.constraintlayout.widget.ConstraintLayout>